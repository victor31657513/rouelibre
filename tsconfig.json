{
"compilerOptions": {
"target": "ES2020",
  "module": "ESNext",
  "moduleResolution": "Bundler",
  "lib": ["ES2020", "DOM", "WebWorker"],
  "strict": true,
  "jsx": "react-jsx",
  "noEmit": true,
  "types": ["three", "vite/client"],
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "baseUrl": ".",
  "paths": {},
  "resolveJsonModule": true
},
"include": ["src"]
}
